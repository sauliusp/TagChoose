{
  "name": "bookmark-ai-autotagger",
  "version": "1.0.0",
  "description": "AI-powered bookmark auto-tagging Chrome extension",
  "scripts": {
    "dev": "parcel watch src/background/background.ts src/index.tsx --no-cache --no-hmr",
    "build": "parcel build src/background/background.ts src/index.tsx --no-cache && npm run copy",
    "copy": "cp -r public dist && cp manifest.json dist && cp popup.html dist && cp reset.css dist",
    "lint": "eslint",
    "fix": "eslint --fix",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/roboto": "^5.1.0",
    "@mui/icons-material": "^6.1.7",
    "@mui/material": "^6.1.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@parcel/config-default": "^2.11.0",
    "@parcel/transformer-typescript-tsc": "^2.11.0",
    "@types/chrome": "^0.0.260",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^9.14.0",
    "eslint-plugin-react": "^7.37.2",
    "fs-extra": "^11.2.0",
    "globals": "^15.12.0",
    "husky": "^9.1.6",
    "parcel": "^2.11.0",
    "process": "^0.11.10",
    "stream-http": "^3.2.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.14.0"
  }
}
